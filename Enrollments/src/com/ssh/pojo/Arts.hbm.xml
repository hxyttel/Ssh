<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<!-- 艺考报名 -->
	<class name="com.ssh.pojo.Arts" table="arts">
		<id name="asid" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="asname" type="java.lang.String"></property>
		<property name="aspicture" type="java.lang.String"></property>
		<property name="asaddress" type="java.lang.String"></property>
		<property name="astitle" type="java.lang.String"></property>
		<property name="asphoto1" type="java.lang.String"></property>
		<property name="asphoto2" type="java.lang.String"></property>
	</class>
</hibernate-mapping>