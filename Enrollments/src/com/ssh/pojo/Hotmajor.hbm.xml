<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.pojo.Hotmajor" table="hotmajor">
		<id name="hid" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="htitle" type="java.lang.String"></property>
		<property name="hpicture" type="java.lang.String"></property>
		<property name="hcontent" type="java.lang.String"></property>
	</class>
</hibernate-mapping>