<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<!-- 联系表 -->
	<class name="com.ssh.pojo.Contact" table="contact">
		<id name="cid" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="ctype" type="java.lang.String"></property>
		<property name="cqqnumber" type="java.lang.String"></property>
		<property name="cphone" type="java.lang.String"></property>
		<property name="cpeople" type="java.lang.String"></property>
	</class>
</hibernate-mapping>