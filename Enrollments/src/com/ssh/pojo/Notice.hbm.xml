<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ssh.pojo.Notice" table="notice">
		<id name="nid" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="ntitle" type="java.lang.String"></property>
		<property name="ncontent" type="java.lang.String"></property>
		<property name="ndate" type="java.lang.String"></property>
		<property name="nflag" type="java.lang.String"></property>
	</class>
</hibernate-mapping>