<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<!-- 职业资格报名 -->
	<class name="com.ssh.pojo.Vocational" table="vocational">
		<id name="vlid" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="vlname" type="java.lang.String"></property>
		<property name="vlpicture" type="java.lang.String"></property>
		<property name="vladdress" type="java.lang.String"></property>
		<property name="vltitle" type="java.lang.String"></property>
		<property name="vlphoto1" type="java.lang.String"></property>
		<property name="vlphoto2" type="java.lang.String"></property>
	</class>
</hibernate-mapping>